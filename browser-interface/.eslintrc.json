{
  "extends": "@dcl/eslint-config/sdk",
  "parserOptions": {
    "project": [
      "tsconfig.eslint.json"
    ]
  },
  "rules": {
    "prettier/prettier": ["error", {
      "printWidth": 120,
      "semi": false,
      "singleQuote": true,
      "trailingComma": "none",
      "tabWidth": 2
    }]
  }
}